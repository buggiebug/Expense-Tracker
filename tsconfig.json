{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["react", "react-native"],
    "skipLibCheck": true,
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/(tabs)/index.jsx", "components/Error.jsx"  ]
}
